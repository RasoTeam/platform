<!DOCTYPE html>
<html>
<head>
  <title>Platform</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if super_user_signed_in? %>
    <%= current_super_user.name %> | 
    <%= link_to "Sign Out", supersignout_path, method: "delete" %>
  <% else %>
    <%= link_to "Sign In", supersignin_path %>
  <% end %>
<%= yield %>
<%= debug(params) if Rails.env.development? %>
</body>
</html>

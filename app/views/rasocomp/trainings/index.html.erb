<h1>TRAININGS#INDEX</h1>
		<% @trainings.each do |t| %>
		  Training id: <%= t.id %>;
		  Name: <%= t.name %>;
		  Desc: <%= t.desc %>;
		  <%= link_to 'start a course', new_company_training_course_path( @company, t) %><br>
		  <%= render :partial => "/rasocomp/courses/index", :locals => {:courses => t.courses} %> 
		  <br/>
		<% end %>